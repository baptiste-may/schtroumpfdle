generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Peoples {
  id            Int      @id @default(autoincrement())
  name          String   @db.TinyText
  sex           Int
  species       String   @db.TinyText
  ennemies      String @db.Text
  looks         String   @db.Text
  first_episode DateTime @db.Date
  img           String   @db.Text
}
